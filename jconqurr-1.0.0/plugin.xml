<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IProject"
            id="org.eclipse.jconqurr.ui.contribution1">
         <menu
               label="Jconqurr"
               path="additions"
               id="org.eclipse.jconqurr.ui.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="Build Parallel Project"
               class="org.eclipse.jconqurr.internal.ui.actions.ConvertParallelAction"
               menubarPath="org.eclipse.jconqurr.ui.menu1/group1"
               enablesFor="1"
               id="org.eclipse.jconqurr.ui.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="Jconqurr">
            <command
                  commandId="org.eclipse.jconqurr.ui.actions.convertparallel"
                  label="Build Parallel Project"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.jconqurr.ui.actions.convertparallel"
            name="Convert Parallel">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.jconqurr.internal.ui.commands.ConvertParallelCommand"
            commandId="org.eclipse.jconqurr.ui.actions.convertparallel">
      </handler>
   </extension>

</plugin>
