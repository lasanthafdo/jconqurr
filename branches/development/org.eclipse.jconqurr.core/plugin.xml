<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.eclipse.jconqurr.core.parallel.convert"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.eclipse.jconqurr.core.parallel.Convert"
            commandId="org.eclipse.jconqurr.core.parallel.convert">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.jdt.ui.PackageExplorer">
         <menu
               label="%menu.label">
            <command
                  commandId="org.eclipse.jconqurr.core.parallel.convert"
                  label="%command.label"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>

</plugin>
